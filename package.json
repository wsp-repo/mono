{
  "name": "@tmk/core-monorepo",
  "description": "TMK",
  "license": "UNLICENSED",
  "version": "1.0.2",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.8.0"
  },
  "workspaces": [
    "service1",
    "service2",
    "shared/*"
  ],
  "scripts": {
    "clean": "npm run clean --workspaces",
    "build": "npm run build:api-sdk && npm run build:services",
    "build:api-sdk": "npm run build --workspace=shared/api-sdk",
    "build:services": "npm run build --workspace=service1 --workspace=service2",
    "start:dev": "concurrently \"npm run start:dev:service1\" \"npm run start:dev:service2\"",
    "start:dev:service1": "npm run start:dev --workspace=service1",
    "start:dev:service2": "npm run start:dev --workspace=service2",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sinclair/typebox": "0.34.38",
    "@sinclair/typemap": "0.10.1",
    "axios": "1.13.2",
    "tsconfig-paths": "4.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "9.9.0",
    "@jest-performance-reporter/core": "2.1.3",
    "@nestjs/cli": "11.0.14",
    "@types/cookie": "0.6.0",
    "@types/cookie-signature": "1.1.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "@types/p-queue": "3.1.0",
    "@types/passport-local": "1.0.38",
    "@types/pg": "8.11.10",
    "@types/qs": "6.14.0",
    "concurrently": "^9.2.1",
    "cross-env": "7.0.3",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-extended": "6.0.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.2.7",
    "pino-pretty": "11.0.0",
    "rimraf": "4.4.1",
    "testcontainers": "11.2.1",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.2"
  }
}
